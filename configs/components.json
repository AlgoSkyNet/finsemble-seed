{
    "comment": "this file contains the components that will be loaded into Finsemble.",
    "components": {
        "accountDetail": {
            "window": {
                "url": "/components/accountDetail/accountDetail.html",
                "width": 800,
                "height": 600
            },
            "component": {
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "accountDetail"
                    }
                }
            }
        },
        "accountList": {
            "window": {
                "url": "/components/accountList/accountList.html",
                "width": 800,
                "height": 600
            },
            "component": {
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "accountList"
                    }
                }
            }
        },
        "accountStatement": {
            "window": {
                "url": "/components/accountStatement/accountStatement.html",
                "width": 800,
                "height": 600
            },
            "component": {
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "accountStatement"
                    }
                }
            }
        },
        "Toolbar": {
            "window": {
                "url": "/components/system/toolbar/toolbar.html",
                "height": 40,
                "top": 0,
                "left": 0,
                "right": 0,
                "position": "available",
                "claimMonitorSpace": true,
                "options": {
                    "resizable": false,
                    "showTaskbarIcon": false
                }
            },
            "component": {
                "spawnOnStartup": true,
                "spawnOnAllMonitors": true
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": false
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
                    },
                    "Window Manager": {
                        "persistWindowState": false,
                        "FSBLHeader": false
                    }
                }
            }
        },
        "Finsemble Documentation": {
            "window": {
                "url": "https://documentation.chartiq.com/finsemble/tutorial-gettingStarted.html",
                "height": 800,
                "width": 1000
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "persistWindowState": true,
                        "FSBLHeader": true,
                        "showLinker": false
                    }
                }
            }
        }
    }
